{
    "version": "0.2",
    "requires": ["jsonlines"],
    "dumper": {
        "data_url": [
            "https://stars.renci.org/var/data_services/translator-ingests/kgx/ctd/latest/ctd.tar.xz"
        ],
        "uncompress": false,
        "release": "hub.dataload.metadata_parser:get_release_for_renci_kgs",
        "schedule": "0 0 * * *"
    },
    "uploaders": [
      {
            "name" : "ctd_nodes",
            "parser": "hub.dataload.dingo_loader:parser",
            "parser_kwargs": {
                "entity": "nodes"
            },
            "on_duplicates": "error",
            "mapping": "hub.dataload.mapping.dingo_mapping:nodes_mapping"
      },
      {
            "name" : "ctd_edges",
            "parser": "hub.dataload.dingo_loader:parser",
            "parser_kwargs": {
                "entity": "edges"
            },
            "on_duplicates": "error",
            "mapping": "hub.dataload.mapping.dingo_mapping:merged_edges_mapping"
      }
    ]
}
