{
    "version": "0.2",
    "requires": [
        "jsonlines"
    ],
    "__metadata__": {
        "graph": "https://stars.renci.org/var/translator/releases/translator_kg/latest/graph-metadata.json",
        "release": "https://stars.renci.org/var/translator/releases/translator_kg/latest-release.json"
    },
    "dumper": {
        "data_url": [
            "https://stars.renci.org/var/translator/releases/translator_kg/latest/translator_kg.tar.zst"
        ],
        "uncompress": false,
        "release": "hub.dataload.metadata_parser:get_kgx_release"
    },
    "uploaders": [
        {
            "name": "tier0_kg_nodes",
            "parser": "hub.dataload.kg_loader:parser",
            "parser_kwargs": {
                "entity": "nodes"
            },
            "on_duplicates": "error",
            "mapping": "hub.dataload.mapping.kg_mapping:nodes_mapping"
        },
        {
            "name": "tier0_kg_edges",
            "parser": "hub.dataload.kg_loader:parser",
            "parser_kwargs": {
                "entity": "edges",
                "gen_seq": false
            },
            "on_duplicates": "error",
            "mapping": "hub.dataload.mapping.kg_mapping:merged_edges_mapping"
        }
    ]
}